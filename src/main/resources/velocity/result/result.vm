#parse("/velocity/layout.vm")
#@mainLayout()
<div id = "content">	
<h1>Results</h1>
	#if($persons)
		#foreach($person in $persons) 
		<div>
	        <a class="shows" href="/persons/$person.personId">    
	            $person.fullName
	        </a><br>
	    </div>	
		#end
	#else
	<h2>No one matched your search</h2>	
	#end
</div>
#end